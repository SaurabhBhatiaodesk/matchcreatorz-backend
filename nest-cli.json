{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/user/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/user/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/user",
  "projects": {
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json",
        "assets": [
          {
            "include": "i18n/**/*",
            "watchAssets": true,
            "outDir": "dist/apps/user"
          },
          {
            "include": "views/**/*",
            "watchAssets": true,
            "outDir": "dist/apps/user"
          },
          {
            "include": "mail/templates/**/*",
            "watchAssets": true,
            "outDir": "dist/apps/user"
          }
        ]
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json",
        "assets": [
          {
            "include": "i18n/**/*",
            "watchAssets": true,
            "outDir": "dist/apps/admin"
          },
          {
            "include": "/mail/templates/**/*",
            "watchAssets": true,
            "outDir": "dist/apps/admin"
          }
        ]
      }
    },
    "socket": {
      "type": "application",
      "root": "apps/socket",
      "entryFile": "main",
      "sourceRoot": "apps/socket/src",
